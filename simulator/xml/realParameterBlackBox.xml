<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE simulator [
<!ATTLIST algorithm id ID #IMPLIED>
<!ATTLIST problem id ID #IMPLIED>
<!ATTLIST measurements id ID #IMPLIED>
]>
<simulator>
    <algorithms>
        <algorithm id="pso" class="pso.PSO">
            <addStoppingCondition class="stoppingcondition.MeasuredStoppingCondition" />
        </algorithm>
    </algorithms>

    <problems>
        <!-- 1.1 Sphere Function -->
        <problem id="f1" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                <function class="functions.continuous.unconstrained.Spherical" />
                <horizontalShift class="controlparameter.FixedControlParameter">
                    <controlParameter class="controlparameter.RandomControlParameter">
                        <distribution class="math.random.UniformDistribution">
                            <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                            <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                        </distribution>
                    </controlParameter>
                </horizontalShift>
            </function>
        </problem>

        <!-- 1.2 Ellipsoidal Function -->
        <problem id="f2" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                <function class="functions.continuous.unconstrained.Elliptic" />
                <horizontalShift class="controlparameter.FixedControlParameter">
                    <controlParameter class="controlparameter.RandomControlParameter">
                        <distribution class="math.random.UniformDistribution">
                            <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                            <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                        </distribution>
                    </controlParameter>
                </horizontalShift>
            </function>
        </problem>

        <!-- 1.3 Rastrigin Function -->
        <problem id="f3" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                <function class="functions.continuous.decorators.IrregularFunctionDecorator">
                    <function class="functions.continuous.decorators.AsymmetricFunctionDecorator">
                        <beta class="controlparameter.ConstantControlParameter" parameter="0.2" />
                        <function class="functions.continuous.decorators.IllConditionedFunctionDecorator">
                            <alpha class="controlparameter.ConstantControlParameter" parameter="10" />
                            <function class="functions.continuous.unconstrained.Rastrigin" />
                        </function>
                    </function>
                </function>
                <horizontalShift class="controlparameter.FixedControlParameter">
                    <controlParameter class="controlparameter.RandomControlParameter">
                        <distribution class="math.random.UniformDistribution">
                            <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                            <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                        </distribution>
                    </controlParameter>
                </horizontalShift>
            </function>
        </problem>

        <!-- 1.4 Buche-Rastrigin Function -->
        <problem id="f4" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.hybrid.SimpleHybridFunction">
                <addFunction class="functions.continuous.decorators.ScaledFunctionDecorator">
                    <verticalScale class="controlparameter.ConstantControlParameter" parameter="100" />
                    <function class="functions.continuous.Penalty">
                        <boundary class="controlparameter.ConstantControlParameter" parameter="5" />
                    </function>
                </addFunction>
                <addFunction class="functions.continuous.decorators.ShiftedFunctionDecorator">
                    <function class="functions.continuous.decorators.IrregularFunctionDecorator">
                        <function class="functions.continuous.unconstrained.modified.BucheRastrigin" />
                    </function>
                    <horizontalShift class="controlparameter.FixedControlParameter">
                        <controlParameter class="controlparameter.RandomControlParameter">
                            <distribution class="math.random.UniformDistribution">
                                <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                                <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                            </distribution>
                        </controlParameter>
                    </horizontalShift>
                </addFunction>
            </function>
        </problem>

        <!-- 1.5 Linear Slope -->
        <problem id="f5" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.unconstrained.modified.LinearSlope">
                <horizontalShift class="controlparameter.FixedControlParameter">
                    <controlParameter class="controlparameter.RandomControlParameter">
                        <distribution class="math.random.UniformDistribution">
                            <lowerBound class="controlparameter.ConstantControlParameter" parameter="-5.0" />
                            <upperBound class="controlparameter.ConstantControlParameter" parameter="5.0" />
                        </distribution>
                    </controlParameter>
                </horizontalShift>
            </function>
        </problem>

        <!-- 2.6 Attractive Sector Function -->
        <problem id="f6" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                    <function class="functions.continuous.decorators.IllConditionedFunctionDecorator">
                        <alpha class="controlparameter.ConstantControlParameter" parameter="10" />
                        <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                            <function class="functions.continuous.unconstrained.modified.AttractiveSector">
                                <horizontalShift class="controlparameter.ConstantControlParameter" parameter="4.0" />
                            </function>
                        </function>
                    </function>
                </function>
                <horizontalShift class="controlparameter.ConstantControlParameter" parameter="4.0" />
            </function>
        </problem>

        <!-- 2.7 Step Ellipsoidal Function -->
        <problem id="f7" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.hybrid.SimpleHybridFunction">
                <addFunction class="functions.continuous.Penalty">
                    <boundary class="controlparameter.ConstantControlParameter" parameter="5" />
                </addFunction>
                <addFunction class="functions.continuous.decorators.ShiftedFunctionDecorator">
                    <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                        <function class="functions.continuous.decorators.IllConditionedFunctionDecorator">
                            <alpha class="controlparameter.ConstantControlParameter" parameter="10" />
                            <function class="functions.continuous.unconstrained.modified.StepEllipsoidal" />
                        </function>
                    </function>
                    <horizontalShift class="controlparameter.FixedControlParameter">
                        <controlParameter class="controlparameter.RandomControlParameter">
                            <distribution class="math.random.UniformDistribution">
                                <lowerBound class="controlparameter.ConstantControlParameter" parameter="-5.0" />
                                <upperBound class="controlparameter.ConstantControlParameter" parameter="5.0" />
                            </distribution>
                        </controlParameter>
                    </horizontalShift>
                </addFunction>
            </function>
        </problem>

        <!-- 2.8 Rosenbrock Function, original -->
        <problem id="f8" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                <function class="functions.continuous.decorators.ScaledFunctionDecorator">
                    <!-- vertical scale is max(1, sqrt(D) / 8) -->
                    <verticalScale class="controlparameter.ConstantControlParameter" parameter="1" />
                    <function class="functions.continuous.decorators.ScaledFunctionDecorator">
                        <horizontalScale class="controlparameter.ConstantControlParameter" parameter="1" />
                        <function class="functions.continuous.unconstrained.Rosenbrock" />
                    </function>
                </function>
                <horizontalShift class="controlparameter.FixedControlParameter">
                    <controlParameter class="controlparameter.RandomControlParameter">
                        <distribution class="math.random.UniformDistribution">
                            <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                            <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                        </distribution>
                    </controlParameter>
                </horizontalShift>
            </function>
        </problem>

        <!-- 2.9 Rosenbrock Function rotated -->
        <problem id="f9" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                <function class="functions.continuous.decorators.ScaledFunctionDecorator">
                    <!-- vertical scale is max(1, sqrt(D) / 8) -->
                    <verticalScale class="controlparameter.ConstantControlParameter" parameter="1" />
                    <function class="functions.continuous.decorators.ScaledFunctionDecorator">
                        <horizontalScale class="controlparameter.ConstantControlParameter" parameter="0.5" />
                        <function class="functions.continuous.unconstrained.Rosenbrock" />
                    </function>
                </function>
            </function>
        </problem>

        <!-- 3.10 Ellipsoidal Function -->
        <problem id="f10" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                    <function class="functions.continuous.decorators.IrregularFunctionDecorator">
                        <function class="functions.continuous.unconstrained.Elliptic" />
                    </function>
                </function>
                <horizontalShift class="controlparameter.FixedControlParameter">
                    <controlParameter class="controlparameter.RandomControlParameter">
                        <distribution class="math.random.UniformDistribution">
                            <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                            <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                        </distribution>
                    </controlParameter>
                </horizontalShift>
            </function>
        </problem>

        <!-- 3.11 Discus Function -->
        <problem id="f11" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                    <function class="functions.continuous.decorators.IrregularFunctionDecorator">
                        <function class="functions.continuous.unconstrained.Discus" />
                    </function>
                </function>
                <horizontalShift class="controlparameter.FixedControlParameter">
                    <controlParameter class="controlparameter.RandomControlParameter">
                        <distribution class="math.random.UniformDistribution">
                            <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                            <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                        </distribution>
                    </controlParameter>
                </horizontalShift>
            </function>
        </problem>

        <!-- 3.12 Bent Cigar Function -->
        <problem id="f12" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                    <function class="functions.continuous.decorators.AsymmetricFunctionDecorator">
                        <beta class="controlparameter.ConstantControlParameter" parameter="0.5" />
                        <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                            <function class="functions.continuous.unconstrained.BentCigar" />
                        </function>
                    </function>
                </function>
                <horizontalShift class="controlparameter.FixedControlParameter">
                    <controlParameter class="controlparameter.RandomControlParameter">
                        <distribution class="math.random.UniformDistribution">
                            <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                            <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                        </distribution>
                    </controlParameter>
                </horizontalShift>
            </function>
        </problem>

        <!-- 3.13 Sharp Ridge Function -->
        <problem id="f13" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                    <function class="functions.continuous.decorators.IllConditionedFunctionDecorator">
                        <beta class="controlparameter.ConstantControlParameter" parameter="10" />
                        <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                            <function class="functions.continuous.unconstrained.DifferentPowers" />
                        </function>
                    </function>
                </function>
                <horizontalShift class="controlparameter.FixedControlParameter">
                    <controlParameter class="controlparameter.RandomControlParameter">
                        <distribution class="math.random.UniformDistribution">
                            <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                            <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                        </distribution>
                    </controlParameter>
                </horizontalShift>
            </function>
        </problem>

        <!-- 3.14 Different Powers Function -->
        <problem id="f14" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                    <function class="functions.continuous.unconstrained.DifferentPowers" />
                </function>
                <horizontalShift class="controlparameter.FixedControlParameter">
                    <controlParameter class="controlparameter.RandomControlParameter">
                        <distribution class="math.random.UniformDistribution">
                            <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                            <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                        </distribution>
                    </controlParameter>
                </horizontalShift>
            </function>
        </problem>

        <!-- 4.15 Rastrigin Function -->
        <problem id="f15" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                    <function class="functions.continuous.decorators.IrregularFunctionDecorator">
                        <function class="functions.continuous.decorators.AsymmetricFunctionDecorator">
                            <beta class="controlparameter.ConstantControlParameter" parameter="0.2" />
                            <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                                <function class="functions.continuous.decorators.IllConditionedFunctionDecorator">
                                    <alpha class="controlparameter.ConstantControlParameter" parameter="10" />
                                    <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                                        <function class="functions.continuous.unconstrained.Rastrigin" />
                                    </function>
                                </function>
                            </function>
                        </function>
                    </function>
                </function>
                <horizontalShift class="controlparameter.FixedControlParameter">
                    <controlParameter class="controlparameter.RandomControlParameter">
                        <distribution class="math.random.UniformDistribution">
                            <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                            <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                        </distribution>
                    </controlParameter>
                </horizontalShift>
            </function>
        </problem>

        <!-- 4.16 Weierstrass Function -->
        <problem id="f16" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.hybrid.SimpleHybridFunction">
                <addFunction class="functions.continuous.decorators.ScaledFunctionDecorator">
                    <!-- vertical scale of penalty is 1 / D -->
                    <verticalScale class="controlparameter.ConstantControlParameter" parameter="1" />
                    <function class="functions.continuous.Penalty">
                        <boundary class="controlparameter.ConstantControlParameter" parameter="5" />
                    </function>
                </addFunction>
                <addFunction class="functions.continuous.decorators.ShiftedFunctionDecorator">
                    <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                        <function class="functions.continuous.decorators.IrregularFunctionDecorator">
                            <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                                <function class="functions.continuous.decorators.IllConditionedFunctionDecorator">
                                    <alpha class="controlparameter.ConstantControlParameter" parameter="0.01" />
                                    <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                                        <function class="functions.continuous.unconstrained.Schaffer7" />
                                    </function>
                                </function>
                            </function>
                        </function>
                    </function>
                    <horizontalShift class="controlparameter.FixedControlParameter">
                        <controlParameter class="controlparameter.RandomControlParameter">
                            <distribution class="math.random.UniformDistribution">
                                <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                                <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                            </distribution>
                        </controlParameter>
                    </horizontalShift>
                </addFunction>
            </function>
        </problem>

        <!-- 4.17 Schaffers F7 Function -->
        <problem id="f17" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.hybrid.SimpleHybridFunction">
                <addFunction class="functions.continuous.decorators.ScaledFunctionDecorator">
                    <verticalScale class="controlparameter.ConstantControlParameter" parameter="10" />
                    <function class="functions.continuous.Penalty">
                        <boundary class="controlparameter.ConstantControlParameter" parameter="5" />
                    </function>
                </addFunction>
                <addFunction class="functions.continuous.decorators.ShiftedFunctionDecorator">
                    <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                        <function class="functions.continuous.decorators.AsymmetricFunctionDecorator">
                            <beta class="controlparameter.ConstantControlParameter" parameter="0.5" />
                            <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                                <function class="functions.continuous.decorators.IllConditionedFunctionDecorator">
                                    <alpha class="controlparameter.ConstantControlParameter" parameter="10" />
                                    <function class="functions.continuous.unconstrained.Schaffer7" />
                                </function>
                            </function>
                        </function>
                    </function>
                    <horizontalShift class="controlparameter.FixedControlParameter">
                        <controlParameter class="controlparameter.RandomControlParameter">
                            <distribution class="math.random.UniformDistribution">
                                <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                                <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                            </distribution>
                        </controlParameter>
                    </horizontalShift>
                </addFunction>
            </function>
        </problem>

        <!-- 4.18 Schaffers F7 Function, moderately ill-conditioned -->
        <problem id="f18" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.hybrid.SimpleHybridFunction">
                <addFunction class="functions.continuous.decorators.ScaledFunctionDecorator">
                    <verticalScale class="controlparameter.ConstantControlParameter" parameter="10" />
                    <function class="functions.continuous.Penalty">
                        <boundary class="controlparameter.ConstantControlParameter" parameter="5" />
                    </function>
                </addFunction>
                <addFunction class="functions.continuous.decorators.ShiftedFunctionDecorator">
                    <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                        <function class="functions.continuous.decorators.AsymmetricFunctionDecorator">
                            <beta class="controlparameter.ConstantControlParameter" parameter="0.5" />
                            <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                                <function class="functions.continuous.decorators.IllConditionedFunctionDecorator">
                                    <alpha class="controlparameter.ConstantControlParameter" parameter="1000" />
                                    <function class="functions.continuous.unconstrained.Schaffer7" />
                                </function>
                            </function>
                        </function>
                    </function>
                    <horizontalShift class="controlparameter.FixedControlParameter">
                        <controlParameter class="controlparameter.RandomControlParameter">
                            <distribution class="math.random.UniformDistribution">
                                <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                                <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                            </distribution>
                        </controlParameter>
                    </horizontalShift>
                </addFunction>
            </function>
        </problem>

        <!-- 4.19 Composite Griewank-Rosenbrock Function F8F2 -->
        <problem id="f19" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                <function class="functions.continuous.decorators.ScaledFunctionDecorator">
                    <!-- vertical scale is max(1, sqrt(D) / 8) -->
                    <verticalScale class="controlparameter.ConstantControlParameter" parameter="1" />
                    <function class="functions.continuous.decorators.ShiftedFunctionDecorator">
                        <horizontalShift class="controlparameter.ConstantControlParameter" parameter="0.5" />
                        <function class="functions.continuous.unconstrained.modified.GriewankRosenbrock" />
                    </function>
                </function>
            </function>
        </problem>

        <!-- 5.20 Schwefel Function -->
        <problem id="f20" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <!-- shifting and ill-conditioning is done within this function -->
            <function class="functions.continuous.unconstrained.modified.Schwefel" />
        </problem>

        <!-- 5.21 Gallagher's Gaussian 101-me Peaks Function -->

        <!-- 5.22 Gallagher's Gaussian 21-hi Peaks Function -->
        <problem id="f22" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <!-- shifting and ill-conditioning is done within this function -->
            <function class="functions.continuous.unconstrained.modified.GallagherGaussianPeaks" />
        </problem>

        <!-- 5.23 Katsuura Function -->
        <problem id="f23" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.hybrid.SimpleHybridFunction">
                <addFunction class="functions.continuous.Penalty">
                    <boundary class="controlparameter.ConstantControlParameter" parameter="5" />
                </addFunction>
                <addFunction class="functions.continuous.decorators.ShiftedFunctionDecorator">
                    <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                        <function class="functions.continuous.decorators.IllConditionedFunctionDecorator">
                            <alpha class="controlparameter.ConstantControlParameter" parameter="100" />
                            <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                                <function class="functions.continuous.unconstrained.Schaffer7" />
                            </function>
                        </function>
                    </function>
                    <horizontalShift class="controlparameter.FixedControlParameter">
                        <controlParameter class="controlparameter.RandomControlParameter">
                            <distribution class="math.random.UniformDistribution">
                                <lowerBound class="controlparameter.ConstantControlParameter" parameter="-4.0" />
                                <upperBound class="controlparameter.ConstantControlParameter" parameter="4.0" />
                            </distribution>
                        </controlParameter>
                    </horizontalShift>
                </addFunction>
            </function>
        </problem>

        <!-- 5.24 Lunacek bi-Rastrigin Function -->
        <problem id="f24" class="problem.FunctionOptimisationProblem" domain="R(-5:5)^10">
            <function class="functions.continuous.hybrid.SimpleHybridFunction">
                <addFunction class="functions.continuous.decorators.ScaledFunctionDecorator">
                    <verticalScale class="controlparameter.ConstantControlParameter" parameter="1E4" />
                    <function class="functions.continuous.Penalty">
                        <boundary class="controlparameter.ConstantControlParameter" parameter="5" />
                    </function>
                </addFunction>
                <addFunction class="functions.continuous.unconstrained.LunacekBiRastrigin">
                    <rastriginDecorator class="functions.continuous.decorators.RotatedFunctionDecorator">
                        <function class="functions.continuous.decorators.IllConditionedFunctionDecorator">
                            <alpha class="controlparameter.ConstantControlParameter" parameter="100" />
                            <function class="functions.continuous.decorators.RotatedFunctionDecorator">
                                <function class="functions.continuous.unconstrained.ModifiedRastrigin" />
                            </function>
                        </function>
                    </rastriginDecorator>
                    <horizontalShift class="controlparameter.FixedControlParameter">
                        <controlParameter class="controlparameter.RandomControlParameter">
                            <distribution class="math.random.UniformDistribution">
                                <lowerBound class="controlparameter.ConstantControlParameter" parameter="-2.5" />
                                <upperBound class="controlparameter.ConstantControlParameter" parameter="2.5" />
                            </distribution>
                        </controlParameter>
                    </horizontalShift>
                    <horizontalScale value="1.0" />
                </addFunction>
            </function>
        </problem>
    </problems>

    <measurements id="measurements" class="simulator.MeasurementSuite" resolution="100">
        <addMeasurement class="measurement.single.Fitness"/>
    </measurements>

